<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
  <title>weather at <!--stationCity--> <!--stationState--></title>
  <meta http-equiv="refresh" content="150" ;="">
  <style type="text/css">
body {
  background-color: #ffffff;
  font-family: gill sans;
}
h1 {
  font-size: 40%;
  font-weight: normal;
  color: #444444;
  padding-top: 4px;
}
a {
  text-decoration: none;
  color: #30a030;
}
td {
  border: 1px solid #eeeeee;
  padding-left: 8px;
  font-size: 140%;
}
.temp {
  font-size: 250%;
}
.temp_high {
  color: #dd4444;
}
.temp_low {
  color: #4444dd;
}
.hilo_time {
  color: #a0a0a0;
  font-size: 50%;
}
.station_info {
  color: #a0a0a0;
  font-size: 50%;
}
  </style>
</head>
<body>

<table width="100%">
  <tr>
    <td width="60%" rowspan="2">
      <a href="tempdaycomp.png"><span class="temp"><!--outsideTemp-->&deg;</span></a><br>
      <span class="station_info"><!--stationDate--> <!--stationTime--></span>
    </td>
    <td>
      <a href="tempdaycomp.png"><span class="temp_high"><!--hiOutsideTemp-->&deg;</span> <span class="hilo_time"><!--hiOutsideTempTime--></span></a>
    </td>
  </tr>
  <tr>
    <td>
      <a href="tempdaycomp.png"><span class="temp_low"><!--lowOutsideTemp-->&deg;</span> <span class="hilo_time"><!--lowOutsideTempTime--></span></a>
    </td>
  </tr>
</table>

<table width="100%">
  <tr>
    <td width="50%">
      <h1>HUMIDITY</h1>
      <a href="humidday.png"><!--outsideHumidity--><!--humUnit--></a>
    </td>
    <td>
      <h1>DEWPOINT</h1>
      <a href="tempdaycomp.png"><!--outsideDewPt-->&deg;</a>
    </td>
  </tr>
  <tr>
    <td>
      <h1>HEAT INDEX</h1>
      <a href="heatchillcomp.png"><!--outsideHeatIndex-->&deg;</a>
    </td>
    <td>
      <h1>HIGH HEAT INDEX</h1>
      <a href="heatchillcomp.png"><!--hiHeatindex-->&deg;</a>
      <span class="hilo_time"><!--hiHeatindexTime-->
    </td>
  </tr>
  <tr>
    <td>
      <h1>WIND CHILL</h1>
      <a href="heatchillcomp.png"><!--windChill-->&deg;</a>
    </td>
    <td>
      <h1>LOW WIND CHILL</h1>
      <a href="heatchillcomp.png"><!--lowWindchill-->&deg;</a>
      <span class="hilo_time"><!--lowWindchillTime-->
    </td>
  </tr>
  <tr>
    <td>
      <h1>RAINFALL</h1>
      <a href="rainday.png"><!--dailyRain--><span class="unit"><!--rainUnit--></span></a>
    </td>
    <td>
      <h1>HIGH RAIN RATE</h1>
      <a href="rainday.png"><!--hiRainRate--> <span class="unit"><!--rateUnit--></span> <span class="hilo_time"><!--hiRainRateTime--></span></a>
    </td>
  </tr>
  <tr>
    <td>
      <h1>WIND</h1>
      <a href="wdirday.png"><!--windDirection--> <!--windSpeed--><span class="unit"><!--windUnit--></span></a>
    </td>
    <td>
      <h1>HIGH WIND</h1>
      <a href="wspeeddaycomp.png"><!--hiWindSpeed--><span class="unit"><!--windUnit--></span> <span class="hilo_time"><!--hiWindSpeedTime--></span></a>
    </td>
  </tr>
</table>

<table width="100%">
  <tr>
    <td width="50%">
      <a href="<!--localRadarURL-->">RADAR</a>
    </td>
    <td>
      <a href="<!--localForecastURL-->">FORECAST</a>
    </td>
  </tr>
</table>

</body>
</html>
